{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": true,
    "lib": [
      "dom",
      "es2017",
      "esnext"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@display/*": [
        "./src/display/*"
      ],
      "@system/*": [
        "./src/system/*"
      ],
      "thundershock": [
        "./_types/main.d.ts"
      ],
    },
    "plugins": [],
    "rootDir": "./src",
    "strict": true,
    "types": [],
    "typeRoots": [
      "_types",
      "node_modules/@types"
    ],
  },
  "include": [
    "./src/**.ts",
    "src/thundershock.config.ts"
  ],
  "exclude": []
}